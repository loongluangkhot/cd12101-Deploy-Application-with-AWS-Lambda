{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "todo",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1,
      "pattern": "\\S+"
    },
    "dueDate": {
      "type": "string",
      "minLength": 10,
      "maxLength": 10,
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
    }
  },
  "required": [
    "name",
    "dueDate"
  ],
  "additionalProperties": false
}